'''
import shapefile as shp
import matplotlib.pyplot as plt

sf = shp.Reader("Research Project/landsat.shp")

plt.figure()
for shape in sf.shapeRecords():
    x = [i[0] for i in shape.shape.points[:]]
    y = [i[1] for i in shape.shape.points[:]]
    plt.plot(x,y)
plt.show()
'''
from zipfile import ZipFile
from osgeo import gdal
import numpy as np

kmz = ZipFile('Research Project/land2.kmz', 'r')
kml = kmz.open('landsat_ot_c2_l2_6502ca5790f38c8d.kml', 'r').read()

gdal.Rasterize("output.tif", kml, outputSRS = "EPSG:3857", xRes = 10, yRes = -10, attribute = "value", noData = np.nan)